<Window x:Class="DataMartFasta.Views.MainETLWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:v="clr-namespace:DataMartFasta.Views"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        SizeToContent="Height"
        Title="MainWindow" Width="800">

    <StackPanel Orientation="Vertical">
        <TextBlock 
            FontWeight="Bold"
            Margin="20"
            Text="Importante: NO ejecute dos procesos de transpaso a la vez."/>
        <v:ControlETL x:Name="despachosControl"/>
        <v:ControlETL x:Name="stockControl"/>
    </StackPanel>
</Window>
